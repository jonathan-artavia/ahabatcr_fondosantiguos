  <data name="SqlCatalogoAmbosResource" xml:space="preserve">
    <value>SELECT * FROM
(SELECT c.ID, Contenido, Signatura, Fecha, materias.Nombres as Materias, IdSerie, Fichero, Libro, NumCaja, NumTomo, Folio, NumExpediente, NumCarpeta, lugares.Nombres as Lugar, `Año`, `Mes`, `Observaciones`, NULL as `FechaCod`, NULL as FechaOrig, 0 as `Hist`
FROM `tblCatalogo` c
LEFT JOIN ( SELECT Catalogo_ID, GROUP_CONCAT(Nombre ORDER BY cl.ID SEPARATOR ' / ') Nombres
           FROM `tblCatalogoLugar` cl
           LEFT JOIN `tblLugar` l ON l.ID = cl.Lugar_ID
           GROUP BY Catalogo_ID ) lugares ON lugares.Catalogo_ID = c.ID
 LEFT JOIN ( SELECT Catalogo_ID, GROUP_CONCAT(Nombre ORDER BY cm.ID SEPARATOR ' / ') Nombres
           FROM `tblCatalogoMateria` cm
           LEFT JOIN `tblMateria` m ON m.ID = cm.Materia_ID
           GROUP BY Catalogo_ID ) materias ON materias.Catalogo_ID = c.ID
UNION
SELECT ID, Descripcion as Contenido, Signatura, Fecha, Materias, NULL as IdSerie, Fichero, NULL as Libro, NULL as NumCaja, NULL as NumTomo, NULL as Folio, NULL as NumExpediente, NULL as NumCarpeta, Lugar, `Año`, `Mes`, Datos as `Observaciones`, `FechaCod`, `FechaOrig`, 1 as `Hist`
FROM `tblHistCatalogo`) ds
WHERE {0}</value>
